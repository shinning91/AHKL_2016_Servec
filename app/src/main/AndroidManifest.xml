<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bryanchan.angelhackkl2016">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.bryanchan.angelhackkl2016.TestActivity">
        </activity>
        <activity android:name="com.example.bryanchan.angelhackkl2016.PeopleProfile">
        </activity>
        <activity android:name=".MolPay" />
        <activity android:name=".QRGenerator" />
        <activity android:name=".InvoiceGenerator" />
        <activity android:name=".InvoiceReceiver"></activity>
        <activity android:name=".SavedContact"></activity>
    </application>

</manifest>